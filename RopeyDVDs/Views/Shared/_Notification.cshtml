@{
}
@if (TempData["success"] != null)
{
    <script>
        swal("Added!", "@TempData["success"]", "success", {
            button: "OK",
        })
    </script>
}

@if (TempData["update"] != null)
{
    <script>
        swal("Update!", "@TempData["update"]", "success", {
            button: "OK",
        })
    </script>
}

@if (TempData["delete"] != null)
{
    <script>
        swal("Delete!", "@TempData["delete"]", "error", {
            button: "OK",
        })
    </script>
}
